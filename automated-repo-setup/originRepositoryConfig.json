{
    "variables": {
        "variableDelimiter": "$"
    },
    "solutionDeletion": {
        "deleteSolution": false,
        "deleteFileKey": "//deleteFile",
        "deleteParagraphKey": "//delete",
        "replaceKey": "//replace",
        "replaceValue": "throw new UnsupportedOperationException();"
    },
    "warnings": {
        "variableValueWarnings": {
            "activate": true,
            "typeWhiteList": [
                "json",
                "java",
                "md"
            ],
            "ignoreList": [
                "name",
                "type"
            ]
        }
    }
}
